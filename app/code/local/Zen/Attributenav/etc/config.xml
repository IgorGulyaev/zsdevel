<?xml version="1.0"?>
<config>
    <modules>
        <Zen_Attributenav>
            <version>0.0.1</version>
        </Zen_Attributenav>
    </modules>
    <global>
        <blocks>
            <zen_attributenav>
                <class>Zen_Attributenav_Block</class>
            </zen_attributenav>
        </blocks>
        <models>
            <zen_attributenav>
                <class>Zen_Attributenav_Model</class>
            </zen_attributenav>
        </models>
        <helpers>
            <zen_attributenav>
                <class>Zen_Attributenav_Helper</class>
            </zen_attributenav>
        </helpers>
    </global>
    <frontend>
        <routers>
            <attribute>
                <use>standard</use>
                <args>
                    <module>Zen_Attributenav</module>
                    <frontName>attribute</frontName>
                </args>
            </attribute>
        </routers>
        <layout>
            <updates>
                <zen_attributenav>
                    <file>zen_attributenav.xml</file>
                </zen_attributenav>
            </updates>
        </layout>
    </frontend>
</config>
